<h2>学生様　新規登録</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "students/shared/error_messages", resource: resource %>

  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :パスワード %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "new-password" %>
  </div>

  <div class="field">
    <%= f.label :パスワード再入力 %><br />
    <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
  </div>

  <div class="field">
    <%= f.label :大学 %><br />
    <%= f.password_field :university, autocomplete: "new-password" %>
  </div>

  <div class="field">
    <%= f.label :姓 %><br />
    <%= f.password_field :first_name, autocomplete: "new-password" %>
  </div>

  <div class="field">
    <%= f.label :名 %><br />
    <%= f.password_field :last_name, autocomplete: "new-password" %>
  </div>

  <div class="field">
    <%= f.label :紹介文 %><br />
    <%= f.password_field :student_introduction, autocomplete: "new-password" %>
  </div>

  <div class="field">
    <%= f.label :スキル %><br />
    <%= f.password_field :skill, autocomplete: "new-password" %>
  </div>

  <div class="field">
    <%= f.label :年齢 %><br />
    <%= f.password_field :age, autocomplete: "new-password" %>
  </div>

  <div class="field">
    <%= f.label :年次 %><br />
    <%= f.password_field :year, autocomplete: "new-password" %>
  </div>



  <div class="actions">
    <%= f.submit "新規登録" %>
  </div>
<% end %>

<%= render "students/shared/links" %>
